static seq [bool1:Bool, bool2:Bool]printBooleanOps
	[bool1, bool2]XOR[x]
	["Result of XOR: "]PRINT
	[x]PRINTLN
	[bool1, bool2]AND[x]
	["Result of AND: "]PRINT
	[x]PRINTLN
	[bool1, bool2]OR[x]
	["Result of OR: "]PRINT
	[x]PRINTLN
	[bool1, bool2]XNOR[x]
	["Result of XNOR: "]PRINT
	[x]PRINTLN
	[bool1, bool2]NAND[x]
	["Result of NAND: "]PRINT
	[x]PRINTLN
	[bool1, bool2]NOR[x]
	["Result of NOR: "]PRINT
	[x]PRINTLN

inline seq [a:Anything, b:Anything]SWAP[newA:typeof a, newB:typeof b]
	[a]:[newB]
	[b]:[newA]